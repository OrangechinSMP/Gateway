{
    "name": "Fast Swimmer",
    "description": "Even with your humanoid body you were still made for swimming. You move much faster in water than others. You do not drown.",
    "type": "origins:multiple",
    "swim": {
        "type": "origins:attribute",
        "modifier": {
            "id": "*:*",
            "attribute": "additionalentityattributes:generic.water_speed",
            "amount": 0.85,
            "operation": "add_multiplied_base"
        }
    },
    "anti_drown": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:gain_air",
            "value": 20
        },
        "interval": 19
    },
    "death_revoke": {
		"type": "origins:action_on_death",
		"bientity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:target_action",
					"action": {
						"type": "origins:revoke_power",
						"power": "gateway:bow/swim_fast",
						"source": "gateway:nudibranch"
					}
				}
			]
		}
	},
	"respawn_grant": {
		"type": "origins:action_on_callback",
		"entity_action_respawned": {
			"type": "origins:grant_power",
			"power": "gateway:bow/swim_fast",
			"source": "gateway:nudibranch"
		}
	}
}