{
    "name": "Blast Off",
    "description": "You channel your explosive power to launch yourself into the air.",
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "hud_render": {
            "should_render": false
        }
    },
    "nofall": {
        "type": "origins:modify_falling",
        "velocity": 0.08,
        "take_fall_damage": false,
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "resource_deprecation": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_resource",
            "change": -1
        },
        "interval": 24,
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "blast": {
        "type": "apoli:active_self",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:sneaking"
                    }
                ]
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_resource",
                        "change": 1
                    },
                    {
                        "type": "apoli:add_velocity",
                        "set": false,
                        "server": false,
                        "y": 1
                    },
                    {
                        "type": "origins:delay",
                        "ticks": 1,
                        "action": {
                            "type": "apoli:add_velocity",
                            "set": false,
                            "server": false,
                            "y": 0.6
                        }
                    },
                    {
                        "type": "origins:play_sound",
                        "sound": "gateway:origins.milkyz.launch",
                        "pitch": 1.3
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "particle minecraft:explosion ~ ~0.25 ~ 0 0 0 1 1 normal"
                    }
                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "*:*_resource",
                        "change": 1
                    },
                    {
                        "type": "apoli:add_velocity",
                        "set": false,
                        "server": false,
                        "y": 1
                    },
                    {
                        "type": "origins:delay",
                        "ticks": 1,
                        "action": {
                            "type": "apoli:add_velocity",
                            "set": false,
                            "server": false,
                            "y": 0.485
                        }
                    },
                    {
                        "type": "origins:play_sound",
                        "sound": "gateway:origins.milkyz.launch",
                        "pitch": 1.1
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "particle minecraft:explosion ~ ~0.25 ~ 0 0 0 1 1 normal"
                    }
                ]
            }
        },
        "key": {
            "key": "key.origins.primary_active"
        },
        "cooldown": 200,
        "hud_render": {
            "comment": "texture for bar done",
            "sprite_location": "gateway:textures/gui/origins/resource_bar.png",
            "bar_index": 0
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:on_block"
                }
            ]
        }
    },
    "smoke_effects": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "particle minecraft:campfire_cosy_smoke"
        },
        "interval": 2,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_resource",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:on_block",
                    "inverted": true
                }
            ]
        }
    }
}