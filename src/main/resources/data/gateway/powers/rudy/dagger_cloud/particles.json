{
    "type": "origins:multiple",
    "particle": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "function gateway:rudy/dagger_cloud/stand_commands"
        },
        "interval": 1
    },
    "death_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 1200,
        "hud_render": {
            "should_render": false
        },
        "max_action": {
            "type": "origins:execute_command",
            "command": "kill"
        }
    },
    "kill": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_death_timer",
            "change": 1
        },
        "interval": 1
    },
    "damage": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:area_of_effect",
            "radius": 2,
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:damage",
                    "amount": 3,
                    "damage_type": "gateway:dagger"
                }
            },
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:item",
                    "inverted": true
                }
            }
        },
        "interval": 1
    },
    "hitbox_shrink": {
        "type": "origins:attribute",
        "modifier": {
            "id": "*:*",
            "attribute": "additionalentityattributes:generic.hitbox_scale",
            "amount": -1,
            "operation": "add_value"
        }
    }
}